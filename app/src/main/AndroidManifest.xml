        </activity>
        <activity
            android:name=".DirectionActivity"
            android:label="@string/title_activity_direction" >
        </activity>
        <activity
            android:name=".MainFirst"
            android:label="TempatWifiMalang" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DataMain"
            android:label="@string/title_activity_data_main" >
        </activity>
        <activity
            android:name=".EditData"
            android:label="@string/title_activity_delete_data" >
        </activity>
    </application>

</manifest>
